{
  "name": "03-sourcemap-demo",
  "version": "1.0.0",
  "description": "Source Map 实战 Demo",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:all": "npm run build:none && npm run build:eval && npm run build:source-map && npm run build:eval-source-map && npm run build:cheap-module-source-map",
    "build:none": "webpack --mode production --env devtool=false",
    "build:eval": "webpack --mode production --env devtool=eval",
    "build:source-map": "webpack --mode production --env devtool=source-map",
    "build:eval-source-map": "webpack --mode production --env devtool=eval-source-map",
    "build:cheap-module-source-map": "webpack --mode production --env devtool=cheap-module-source-map"
  },
  "keywords": [
    "sourcemap",
    "webpack",
    "debug"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.5.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
