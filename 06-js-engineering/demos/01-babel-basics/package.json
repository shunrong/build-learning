{
  "name": "01-babel-basics",
  "version": "1.0.0",
  "description": "Babel 基础配置 Demo",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:stats": "webpack --mode production --json > stats.json",
    "babel": "babel src/index.js --out-file compiled.js",
    "babel:watch": "babel src/index.js --watch --out-file compiled.js",
    "babel:old": "babel src/index.js --out-file compiled.old.js --config-file ./babel.config.old.js"
  },
  "keywords": [
    "babel",
    "webpack",
    "es6"
  ],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.5.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
