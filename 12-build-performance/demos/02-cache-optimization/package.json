{
  "name": "02-cache-optimization",
  "version": "1.0.0",
  "description": "Webpack 缓存优化对比 Demo",
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache .babel-cache",
    "build:no-cache": "npm run clean && webpack --mode production --config webpack.no-cache.config.js",
    "build:with-cache": "webpack --mode production --config webpack.with-cache.config.js",
    "build:with-cache:first": "npm run clean && npm run build:with-cache",
    "build:with-cache:second": "npm run build:with-cache",
    "compare": "node compare.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
